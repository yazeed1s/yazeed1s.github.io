{
    "name": "Gruvbox Material Light",
    "type": "light",
    "colors": {
        "editor.background": "#151515",
        "editor.foreground": "#3c3836"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#928374",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#9c4641"
            }
        },
        {
            "scope": [
                "keyword.operator",
                "punctuation.accessor"
            ],
            "settings": {
                "foreground": "#9c4641"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#906e3a"
            }
        },
        {
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#742602"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language"
            ],
            "settings": {
                "foreground": "#8c5967"
            }
        },
        {
            "scope": [
                "constant",
                "constant.other"
            ],
            "settings": {
                "foreground": "#8c5967"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#707943"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#576e65"
            }
        },
        {
            "scope": [
                "variable",
                "variable.other"
            ],
            "settings": {
                "foreground": "#3c3836"
            }
        },
        {
            "scope": [
                "variable.parameter",
                "entity.name.variable.parameter",
                "variable.other.member",
                "variable.other.property",
                "variable.other.field"
            ],
            "settings": {
                "foreground": "#3c3836"
            }
        },
        {
            "scope": [
                "entity.name.function.preprocessor",
                "meta.preprocessor",
                "keyword.control.directive"
            ],
            "settings": {
                "foreground": "#955f3c"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#3c3836"
            }
        },
        {
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#955f3c"
            }
        },
        {
            "scope": [
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#685c52"
            }
        },
        {
            "scope": [
                "punctuation",
                "punctuation.separator",
                "punctuation.terminator"
            ],
            "settings": {
                "foreground": "#685c52"
            }
        },
        {
            "scope": [
                "support.function.builtin"
            ],
            "settings": {
                "foreground": "#742602"
            }
        },
        {
            "scope": [
                "entity.name.namespace",
                "entity.name.module"
            ],
            "settings": {
                "foreground": "#955f3c"
            }
        },
        {
            "scope": [
                "entity.other.inherited-class",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#3c3836"
            }
        },
        {
            "scope": [
                "entity.name.label"
            ],
            "settings": {
                "foreground": "#955f3c"
            }
        },
        {
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#707943"
            }
        },
        {
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#9c4641",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#906e3a",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.inline.raw",
                "markup.raw"
            ],
            "settings": {
                "foreground": "#5b7856"
            }
        },
        {
            "scope": [
                "markup.underline.link",
                "markup.link"
            ],
            "settings": {
                "foreground": "#576e65"
            }
        },
        {
            "scope": [
                "markup.quote"
            ],
            "settings": {
                "foreground": "#685c52",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.list"
            ],
            "settings": {
                "foreground": "#9c4641"
            }
        },
        {
            "scope": [
                "markup.strikethrough"
            ],
            "settings": {
                "foreground": "#524a42"
            }
        }
    ]
}